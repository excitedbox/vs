{
    "name": "vde2",
    "version": "1.0.0",
    "description": "Virtual Desktop Environment 2",
    "main": "index.js",
    "scripts": {
        "build": "webpack --mode production",
        "watch": "webpack --mode development --watch",
        "start": "ts-node index.ts",
        "test": "mocha -r ts-node/register test/**/*.test.ts",
        "coverage": "nyc --temp-dir bin/nyc --report-dir bin/public/coverage -r lcov -e .ts -x \"*.test.ts\" npm run test",
        "docs": "typedoc --mode file --out bin/public/docs src",
        "amount": "node amount.js",
        "lint": "eslint src/**/*.ts"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "extends": [
            "plugin:@typescript-eslint/recommended"
        ],
        "env": {
            "node": true
        },
        "parserOptions": {
            "ecmaVersion": 6,
            "sourceType": "module"
        },
        "rules": {
            "semi": "error",
            "curly": "error",
            "@typescript-eslint/no-inferrable-types": "off",
            "@typescript-eslint/camelcase": "off",
            "@typescript-eslint/class-name-casing": "off",
            "@typescript-eslint/interface-name-prefix": "off",
            "@typescript-eslint/typedef": "error",
            "@typescript-eslint/array-type": "error",
            "@typescript-eslint/no-empty-function": "off",
            "@typescript-eslint/no-this-alias": "off",
            "@typescript-eslint/ban-ts-ignore": "off"
        }
    },
    "browserslist": [
        "ie >= 9",
        "last 4 version"
    ],
    "author": "Maldan",
    "license": "ISC",
    "dependencies": {
        "@babel/core": "^7.9.6",
        "@types/chai": "^4.2.7",
        "@types/chai-as-promised": "^7.1.2",
        "@types/express": "^4.17.2",
        "@types/jsdom": "^12.2.4",
        "@types/mocha": "^5.2.7",
        "@types/node": "^13.5.0",
        "@types/react": "^16.9.34",
        "@types/react-dom": "^16.9.7",
        "@types/react-redux": "^7.1.8",
        "@types/rimraf": "^2.0.3",
        "@types/ssh2": "^0.5.41",
        "@types/ws": "^7.2.3",
        "@typescript-eslint/eslint-plugin": "^2.18.0",
        "@typescript-eslint/parser": "^2.18.0",
        "autoprefixer": "^9.7.4",
        "axios": "^0.19.2",
        "babel-loader": "^8.1.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "body-parser": "^1.19.0",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "copy-dir": "^1.2.0",
        "cors": "^2.8.5",
        "dotenv": "^8.2.0",
        "eslint": "^6.8.0",
        "express": "^4.17.1",
        "express-formidable": "^1.2.0",
        "glob": "^7.1.6",
        "jsdom": "^16.0.1",
        "math": "file:src/module/math",
        "md5": "^2.2.1",
        "mime-types": "^2.1.26",
        "mocha": "^7.0.1",
        "ncp": "^2.0.0",
        "node-sass": "^4.13.1",
        "nyc": "^15.0.0",
        "opn": "^6.0.0",
        "postcss": "^7.0.26",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "recursive-readdir": "^2.2.2",
        "redux": "^4.0.5",
        "request": "^2.88.2",
        "rimraf": "^3.0.0",
        "source-map-loader": "^0.2.4",
        "ssh2": "^0.8.9",
        "ts-loader": "^7.0.3",
        "ts-node": "^8.6.2",
        "typedoc": "^0.16.9",
        "typescript": "^3.7.5",
        "typescript-eslint": "0.0.1-alpha.0",
        "vm2": "^3.8.4",
        "vue": "^2.6.11",
        "vue-fragment": "^1.5.1",
        "vue-property-decorator": "^8.4.2",
        "vue-vs-ui": "file:src/module/vue-vs-ui",
        "webpack": "^4.41.5",
        "webpack-cli": "^3.3.10",
        "ws": "^7.2.3"
    }
}
